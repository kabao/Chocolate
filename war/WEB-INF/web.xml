<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Chocolate</servlet-name>
		<servlet-class>tsuda.choco.ChocolateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Chocolate</servlet-name>
		<url-pattern>/chocolate</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ChocoStore</servlet-name>
		<servlet-class>tsuda.choco.ChocoStoreServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChocoStore</servlet-name>
		<url-pattern>/chocostore</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>NewUser</servlet-name>
		<servlet-class>tsuda.choco.NewUserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>NewUser</servlet-name>
	<url-pattern>/import.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Import</servlet-name>
		<servlet-class>tsuda.choco.ImportOK</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>Import</servlet-name>
	<url-pattern>/import2.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ChocoListFromDB</servlet-name>
		<servlet-class>tsuda.choco.ChocoListFrom</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChocoListFromDB</servlet-name>
		<url-pattern>/cart.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>newuser_linkdata</servlet-name>
		<servlet-class>tsuda.choco.NewUserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>newuser_linkdata</servlet-name>
		<url-pattern>/NewUser</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>addNewUser</servlet-name>
	<servlet-class>tsuda.choco.AddLinkDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>addNewUser</servlet-name>
	<url-pattern>/add</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>Welcome.jsp</welcome-file>
	</welcome-file-list>
	
	
</web-app>